{
  "name": "mwangaben-modal",
  "version": "0.0.1",
  "description": "A Simple Vue Modal Plugin",
  "main": "index.js",
  "scripts": {
    "dev": "npm run development",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "npm run development -- --watch",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "npm run production",
    "test": "mocha-webpack --webpack-config=node_modules/laravel-mix/setup/webpack.config.js --require tests/setup.js tests/**/*.spec.js",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mwangaben/mwangaben-modal.git"
  },
  "keywords": [
    "vue modal",
    "modal plugin",
    "mwangaben",
    "mwangaben-modal",
    "modal",
    "vue Plugin"
  ],
  "author": "Benedict Mwanga",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mwangaben/mwangaben-modal/issues"
  },
  "homepage": "https://github.com/mwangaben/mwangaben-modal#readme",
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.31",
    "cross-env": "^7.0.0",
    "jsdom": "^15.2.1",
    "jsdom-global": "^3.0.2",
    "laravel-mix": "^5.0.1",
    "mocha": "^10.2.0",
    "mocha-webpack": "^2.0.0-beta.0",
    "mwangaben-vthelpers": "^1.3.0",
    "resolve-url-loader": "^3.1.1",
    "vue-template-compiler": "^2.6.11"
  },
  "dependencies": {
    "vue": "^2.6.11"
  }
}
